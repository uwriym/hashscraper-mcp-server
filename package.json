{
  "name": "@hashscraper/mcp-server",
  "version": "1.0.3",
  "description": "HashScraper MCP Server - Web scraping for AI agents. Bypass anti-bot systems and get clean, LLM-ready content via MCP protocol.",
  "type": "module",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "inspector": "npx @modelcontextprotocol/inspector node dist/index.js",
    "prepublishOnly": "npm run build && chmod +x dist/index.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "hashscraper",
    "web-scraping",
    "scraping",
    "ai",
    "ai-agent",
    "llm",
    "anti-bot"
  ],
  "author": "Hashscraper",
  "license": "MIT",
  "homepage": "https://www.hashscraper.com/mcp",
  "repository": {
    "type": "git",
    "url": "https://github.com/bamchi/hashscraper-mcp-server.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.0",
    "axios": "^1.7.0",
    "turndown": "^7.2.0",
    "@mozilla/readability": "^0.5.0",
    "jsdom": "^24.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/turndown": "^5.0.4",
    "@types/jsdom": "^21.1.6",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
